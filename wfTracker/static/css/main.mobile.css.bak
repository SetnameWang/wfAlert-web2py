#invaBar
{
    border-style:none;
    height: 10px;
    width: 100%;
	background-color: #f3f3f3;
}
.background-1 p {
    font-size: 50px;
}
.invaBar
{
    border-style:none;
    height: 10px;
    width: 100%;
	background-color: #dedede;
}
.topButton
{
    font-size: 15px;
    font-weight:bold;
    color: #000;
    padding: 4px;
    display: inline-block;
    text-decoration:none;
    transition: 0.5s;
    -moz-transition: 0.5s;	/* Firefox 4 */
    -webkit-transition: 0.5s;	/* Safari 和 Chrome */
    -o-transition: 0.5s;	/* Opera */
}
table td
{
    height:100px;
	font-size:35px;
}
.topButton:hover , .topButton:active
{
    color: #424;
    font-size: 25px;
}
.alertBody
{
    color: #c5c5c5;
    width: 100%;
    margin: 0px;
}
.alertBody ul
{
    padding: 0px;
    width: 100%;
}
.alertBody ul li
{
    margin: 8px 0px;
    width: 100%;
}
.alertBody ul li div
{
    border-style: none;
    border-radius: 8px;
    background-color: #d5d4d8;
    width: 100%;
}
td span
{
    font-size: 32px;
    color: rgba(0, 0, 0, 1);
    display:block;
    float:left;
    margin:0 5px 0 0;
}
p
{
    margin: 0px;
    padding: 10px;
    text-align: center;
    background-color: #212e44;
}
p:hover , p:active
{
    background: #29426b;
}
.items
{
    color: #fff;
    background-color: #b3ae22;
    padding: 0px 4px;
    font-size: 30px;
}
.crs
{
    color: #fff;
    background-color: #777;
    padding: 0px 4px;
    font-size: 30px;
}
.timeLeft
{
    color: #fff;
    background-color: #337ab7;
    padding: 0px 4px;
    font-size: 30px;
}
.leftTd
{
    width: 40%;
}
table
{
    border-collapse:collapse;
    width: 100%;
}
table tr
{
    height: 38px;
    border-top: 2px solid #adadad;
    transition: 0.5s;
    -moz-transition: 0.5s;	/* Firefox 4 */
    -webkit-transition: 0.5s;	/* Safari 和 Chrome */
    -o-transition: 0.5s;	/* Opera */
}
table tr:hover ,table tr:active
{
    background-color: #adadad;
    height: 60px;
}
.location
{
    font-weight: bold;
}
.leftReward
{
    color: #fff;
    background-color: green;
    padding: 0px 4px;
    border-radius: 2px;
    font-size: 30px;
    float: left;
}
.rightReward
{
    color: #fff;
    background-color: green;
    padding: 0px 4px;
    border-radius: 2px;
    font-size: 30px;
