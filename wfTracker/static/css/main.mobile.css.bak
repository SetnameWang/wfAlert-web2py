    border-radius: 4px;
    font-size: 30px;
}
.rewardFrame
{
    background-color:#d4d3d7;
}
#plusOptions
{
    width: 120px;
    height: 120px;
    bottom: 40px;
    position: fixed;
    text-align: center;
    font-size: 80px;
    line-height: 120px;
    background: #f1eef7;
    border-radius: 120px;
    cursor: pointer;
    transition: 0.3s;
    -moz-transition: 0.3s;	/* Firefox 4 */
    -webkit-transition: 0.3s;	/* Safari 和 Chrome */
    -o-transition: 0.3s;	/* Opera */
}
#plusOptions:hover
{
    width: 130px;
    height: 130px;
    line-height: 130px;
    font-size: 50px;
    bottom: 35px;
}
#plusOptionBar
{
    display:none;
    position: fixed;
    display:block;
    bottom: 0px;
    right:0px;
    transition: 0.3s;
    -moz-transition: 0.3s;	/* Firefox 4 */
    -webkit-transition: 0.3s;	/* Safari 和 Chrome */
    -o-transition: 0.3s;	/* Opera */
}
#plusOptionBar ul li
{
    font-size:40px;
    float: none;
    background: #222229cc;
    margin: 8 0 6 auto;
    padding:0 8;
    display:table;
    border-radius:2px;
    line-height:80px;
    text-align:center;
    color:#c4fff9;
}
#plusOptionBar ul li a
{
    text-decoration:none;
    color:#c4fff9;
}
#plusOptionBar img
{
    width:auto;
    margin-bottom: 5px;
}
